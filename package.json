{
  "name": "algorithms-project",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://gvozdenkov.github.io/algososh/",
  "type": "module",
  "imports": {
    "#*": [
      "./src/*",
      "./src/*.ts",
      "./src/*.tsx",
      "./src/*.js",
      "./src/*.jsx",
      "./src/*/index.ts",
      "./src/*/index.tsx",
      "./src/*/index.js",
      "./src/*/index.jsx"
    ]
  },
  "dependencies": {
    "clsx": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-fast-marquee": "^1.6.2",
    "react-router-dom": "^6.18.0"
  },
  "scripts": {
    "start": "vite",
    "dev": "start-test http://localhost:5173 'cypress open'",
    "build": "tsc -p tsconfig.prod.json && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prettier": "prettier --write src .eslintrc.cjs .lintstagedrc.cjs cypress",
    "cy:run": "cypress run",
    "cy:run:record": "cypress run --record",
    "cy:run:chrome": "cypress run --browser chrome",
    "cy:run:record:chrome": "cypress run --record --browser chrome",
    "test:cy": "start-test http://localhost:5173 cy:run",
    "test:jest": "cross-env NODE_ENV=test jest",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d dist"
  },
  "devDependencies": {
    "@swc/core": "^1.3.99",
    "@swc/jest": "^0.2.29",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.9",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-test-renderer": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "@vitejs/plugin-react": "^4.2.0",
    "camelcase": "^6.3.0",
    "cross-env": "^7.0.3",
    "cypress": "^13.6.0",
    "cypress-vite": "^1.5.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "fetch-mock": "^9.11.0",
    "gh-pages": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-watch-typeahead": "^2.2.2",
    "prettier": "2.8.7",
    "react-test-renderer": "^18.2.0",
    "sass": "^1.69.5",
    "start-server-and-test": "^2.0.3",
    "typescript": "5.3",
    "vite": "^4.5.0"
  }
}
